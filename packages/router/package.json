{
  "name": "@arijs/remix-run-router",
  "version": "1.3.4-arijs.2",
  "description": "Nested/Data-driven/Framework-agnostic Routing",
  "keywords": [
    "remix",
    "router",
    "location"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arijs/react-router",
    "directory": "packages/router"
  },
  "license": "MIT",
  "author": "Remix Software <hello@remix.run>",
  "sideEffects": false,
  "main": "./dist/router.cjs.js",
  "unpkg": "./dist/router.umd.min.js",
  "module": "./dist/router.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/router.mjs",
      "umd": "./dist/router.umd.min.js",
      "import": "./dist/router.mjs",
      "require": "./dist/router.cjs.js"
    }
  },
  "files": [
    "dist/",
    "*.ts",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
